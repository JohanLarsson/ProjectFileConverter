    <OutputType>WinExe</OutputType>
    <TargetFramework>net461</TargetFramework>
    <UseWPF>true</UseWPF>
    <CodeAnalysisRuleSet>ProjectFileConverter.ruleset</CodeAnalysisRuleSet>
